<launch>
    <node pkg="tobias" name="TobiasRunner" type="tobiasControl.py"/>
    <node pkg="tobias" name="JoyPipeline" type="joyPipeline.py"/>
    <node pkg="tobias" name="sliders" type="gui.py"/>
    <!--Executive for the test programs-->

    <node pkg="joy" name="joystick" type="joy_node">
    <param name="dev" value="/dev/input/js0" />
    </node>

    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="57600"/>
    </node>

</launch>
